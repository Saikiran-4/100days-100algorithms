#include<bits/stdc++.h>
using namespace std;

void KString()
{
int k, len = 0,c=0;
cin>>k;
int A[26] = {0};
char a;
while (cin>>a)
{
A[a-'a']++;
len++;
}
if (len == 0 || len % k) 
{
cout<<"NO";
return;
}

bool ks = true;
for (int i = 0; i <26; i++)
{
if (A[i] % k != 0) ks = false;
}
if (!ks) cout<<"NO";
else
{
for (int d = 0; d <k; d++)
{
for (int i = 0; i <26; i++)
{
if (A[i])
{
c++;
break;
} 
}
}
if(c)
cout<<"YES"<<endl;
else
cout<<"NO"<<endl;
}
}
int main()
{
KString();
return 0;
}