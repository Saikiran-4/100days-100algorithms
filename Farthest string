#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n,c=0,l;
scanf("%d",&n);
char s[1005];
while(n--)
{
char a[1005];
scanf("%s",a);
int x=0;
int m=strlen(a);
for(int i=0;i<m/2;i++)
if(a[i]!=a[m-i-1])
x++;
if(x>c)
{
c=x;
l=m;
for(int i=0;i<m;i++)
{
s[i]=a[i];
}
}
}
for(int i=0;i<l;i++)
printf("%c",s[i]);
return 0;
}